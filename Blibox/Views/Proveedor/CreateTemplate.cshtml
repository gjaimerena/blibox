                    new
                    {
                        htmlAttributes = new
                        {
                            @class = "form-control",
                            @id = "domicilio",
                            data_fv_row = ".dobleEntrada",

                            data_fv_stringlength_message = "<i class='glyphicon glyphicon-info-sign'></i> Domicilio no válido",
                            data_fv_stringlength_min = 0,
                            data_fv_stringlength = "true"
                        }
                    })
