@model Blibox.Proveedor

<div class="box-body">
    <div class="form-group">
        <div class="dobleEntrada">
            @Html.Label("Razon social", htmlAttributes: new { @class = "control-label col-md-2" })
            <div class="col-md-4">
                <div class="input-group">
                    <div class="input-group-addon"><i class="fa fa-pencil" aria-hidden="true"></i></div>
                    @Html.EditorFor(
                    model => model.Razon_social,
                    new
                    {
                    htmlAttributes = new
                    {
                    @class = "form-control",
                    @id = "razonSocial",
                    data_fv_notempty_message = "<i class='glyphicon glyphicon-info-sign'></i> Debe ingresar la razón social",
                    required = "required",
                    data_fv_row = ".dobleEntrada",
                    autofocus = "autofocus"
                    }
                    })
                </div>
            </div>
        </div>

        <div class="dobleEntrada">
            @Html.LabelFor(model => model.Contacto, htmlAttributes: new { @class = "control-label col-md-2 no-clear-md" })
            <div class="col-md-4">
                <div class="input-group">
                    <div class="input-group-addon"><i class="fa fa-user" aria-hidden="true"></i></div>
                    @Html.EditorFor(
                    model => model.Contacto,
                    new
                    {
                    htmlAttributes = new
                    {
                    @class = "form-control",
                    @id = "contacto",
                    data_fv_notempty_message = "<i class='glyphicon glyphicon-info-sign'></i> Debe ingresar el contacto",
                    required = "required",
                    data_fv_row = ".dobleEntrada",
                    }
                    })
                </div>
            </div>
        </div>
    </div>

