<!-- Content Wrapper. Contains page content -->
<div class="content-wrapper">
    <!-- Content Header (Page header) -->
    <section class="content-header">
        <h1>
            Escritorio <small>Panel de Control</small>
        </h1>
        <ol class="breadcrumb">
            <li><a href="#"><i class="fa fa-dashboard"></i> Escritorio</a></li>
            <li class="active">Inicio</li>
        </ol>
    </section>
    <!-- Main content -->
    <section class="content">
       
        <div class="row">
            <div class="col-lg-4 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-aqua">
                    <div class="inner">
                        <h3>
                            @if (ViewBag.pedidospendientes != null)
                            {@ViewBag.pedidospendientes}
                        </h3>

                        <p>Pedidos pendientes</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-bag"></i>
                    </div>
                    <a href="@Url.Action("Index", "Pedidos")" class="small-box-footer">Más Info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            
            <!-- ./col -->
            <div class="col-lg-4 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-green">
                    <div class="inner">
                        <h3>@if (ViewBag.clientes != null)
                        {@ViewBag.clientes}</h3>

                        <p>Clientes</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-person-add"></i>
                    </div>
                    <a href="@Url.Action("Index", "Clientes")" class="small-box-footer">Más Info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
            <div class="col-lg-4 col-xs-6">
                <!-- small box -->
                <div class="small-box bg-yellow">
                    <div class="inner">
                        <h3>@if (ViewBag.articulos != null)
                        {@ViewBag.articulos}</h3>

                        <p>Articulos</p>
                    </div>
                    <div class="icon">
                        <i class="ion ion-pie-graph"></i>
                    </div>
                    <a href="@Url.Action("Index", "Articulos")" class="small-box-footer">Más Info <i class="fa fa-arrow-circle-right"></i></a>
                </div>
            </div>
            <!-- ./col -->
        </div>

        <div class="box col-md-12">
            <div class="box-body" >
                <p>ACCESOS RAPIDOS</p>
                <a href="@Url.Action("Index", "Articulos")" class="btn btn-app"  style="margin-bottom:10px; font-size:13px;">
                    <span class="badge bg-green">
                        @if (ViewBag.articulos != null)
                    {@ViewBag.articulos}
                </span>
                    <i class="fa fa-edit"></i> Articulos
                </a>
                <a href="@Url.Action("Index", "Clientes")" class="btn btn-app"  style="margin-bottom:10px; font-size:13px;">
                    <span class="badge bg-green">
                        @if (ViewBag.clientes != null)
                    {@ViewBag.clientes}
                </span>
                     <i class="fa fa-user" aria-hidden="true"></i> Clientes
                </a>
                <a href="@Url.Action("Index", "Proveedor")"  class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                    <i class="fa fa-repeat"></i> Proveedores
                </a>
                <a href="@Url.Action("Index", "Pedidos")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                    <span class="badge bg-green">@if (ViewBag.pedidospendientes != null)
                    {@ViewBag.pedidospendientes}</span>
                    <i class="fa fa-file-text"></i> Pedidos
                </a>
                <a href="@Url.Action("Index", "Reportes")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                    <i class="fa fa-save"></i> Reportes
                </a>
                <a href="@Url.Action("Index", "Compras")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                   
                    <i class="fa fa-cart-plus"></i> Compras
                </a>
                <a href="@Url.Action("Index", "CtaCteClientes")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                   
                    <i class="fa fa-bank"></i> Cta Cte Clientes
                </a>
                <a href="@Url.Action("Index", "CtaCteProveedores")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">

                    <i class="fa fa-bank"></i> Cta Cte Proveedores
                </a>
                <a href="@Url.Action("Index", "Factura")" class="btn btn-app" style="margin-bottom:10px; font-size:13px;">
                    
                    <i class="fa fa-money"></i> Facturacion
                </a>
               
            </div>
        </div>

